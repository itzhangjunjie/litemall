/**app.wxss**/

.container {
  box-sizing: border-box;
  background-color: #f4f4f4;
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

view, image, text, navigator {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

view, text {
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
  font-size: 29rpx;
  color: #333;
}
/* 底部logo */
.logobtclass{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 54rpx;
}
.logobtclass image{
  width: 120rpx;
  height: 50rpx;
}
